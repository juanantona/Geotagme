<h1>WELCOME TO GEOTAGME</h1>

<ul>
<% @photos.each do |file| %>
   <% if file.class == Array %>
	   <% file.each do |photo| %>
	     <li><%= link_to "#{photo[:path]}", viewmap_path %></li>
	   <% end %> 
   <% else %>

       <li><%= link_to "#{file[:path]}", viewmap_path %></li>
   <% end %>
<% end %>       

</ul>

